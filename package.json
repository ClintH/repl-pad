{
  "name": "replpad",
  "version": "0.0.2",
  "description": "Javascript REPL",
  "main": "./dist/index.js",
  "type": "module",
  "module": "./dist/index.js",
  "repository": "https://github.com/clinth/replpad",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf build dist",
    "build": "tsup ./src/index.ts --format esm --sourcemap",
    "dev": "tsup ./src/index.ts --format esm --sourcemap --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "repl"
  ],
  "author": "Clint Heyer",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^3.0.2",
    "tsup": "^5.11.13",
    "typescript": "^4.7.0-dev.20220227"
  }
}